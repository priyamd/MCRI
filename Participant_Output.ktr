        <jsScript_script>var DOB=null;
var DOB_cmt=null;

if((Birthdate1 !== null) &amp;&amp; (Birthdate2 !== null))
{
	//if(Birthdate1.setHours(0, 0, 0, 0) === Birthdate2.setHours(0, 0, 0, 0)) 
	if((Birthdate1.getDate() == Birthdate2.getDate()) &amp;&amp; (Birthdate1.getMonth() == Birthdate2.getMonth()) &amp;&amp; (Birthdate1.getYear() == Birthdate2.getYear())) 
	{
		DOB=Birthdate1;
	}
	else
	{
		DOB_cmt="D.O.B. and Patient DOB are diffrent";
	}
}
else 
{
	if ((Birthdate1 == null) &amp;&amp; (Birthdate2 == null))
	{
		DOB_cmt="D.O.B. and Patient DOB are empty";
	}
	else if(Birthdate1 == null)
	{
		DOB=Birthdate2;
	}
	else
	{
		DOB=Birthdate1;
	}
}

var LName=null;
var LName_cmt=null;

if((lname1 != null) &amp;&amp; (lname2 != null))
{
	if(lname1.localeCompare(lname2) == 0) 
	{
		LName = lname1;
	}
	else
	{
		LName_cmt = "Last Name and Patient Last Name are diffrent";
	}
}
else 
{
	if ((lname1 == null) &amp;&amp; (lname2 == null))
	{
		LName_cmt = "Last Name and Patient Last Name are empty";
	}
	else if(lname1 == null)
	{
		LName = lname2;
	}
	else
	{
		LName = lname1;
	}
}

var MRN=null;
if((MRN1 != null) &amp;&amp; (MRN2 != null))
{
		MRN = MRN1;
}
else 
{
	if(MRN1 == null)
	{
		MRN = MRN2;
	}
	else
	{
		MRN = MRN1;
	}
}


</jsScript_script>
